{
  "name": "qwik-speak",
  "description": "Internationalization (i18n) library to translate texts, dates and numbers in Qwik apps",
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "node ./packages/qwik-speak/extract/cli.js --supportedLangs=en-US,it-IT --assetsPath=i18n",
    "start": "vite --open --mode ssr",
    "test": "vitest test --run",
    "test.e2e": "playwright test",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.6",
    "@builder.io/qwik-city": "1.2.6",
    "@playwright/test": "1.35.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/eslint": "8.40.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.7.0",
    "eslint": "8.44.0",
    "eslint-plugin-qwik": "1.2.6",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0",
    "prettier": "3.0.0",
    "qwik-speak": "^0.15.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "undici": "5.22.0",
    "vite": "4.4.1",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@glidejs/glide": "^3.6.0",
    "@modular-forms/qwik": "^0.20.2",
    "@splidejs/splide": "^4.1.4",
    "@unpic/qwik": "^0.0.25"
  }
}
